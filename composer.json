{
    "name": "cotya/composer-test-framework",
    "description": "a framework to easier build tests for composer installer and plugins",
    "license": "MIT",
    "authors": [
        {
            "name": "Flyingmana",
            "email": "flyingmana@googlemail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cotya\\ComposerTestFramework\\": "src/"
        }
    },
    "require": {
        "symfony/process":"*",
        "symfony/filesystem":"*"
    },
    "suggest": {
      "ext-SPL_Types":"*"
    },
    "require-dev": {
        "phpunit/phpunit":"4.*"
    }
}
